{
  "name": "resend-cli",
  "version": "0.1.0",
  "description": "",
  "main": "dist/cli.js",
  "bin": {
    "resend-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "tsx --watch src/cli.tsx",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "format": "biome format --write .",
    "check": "biome check .",
    "check:fix": "biome check --write --unsafe .",
    "prepublishOnly": "npm run build",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "ink": "^6.0.1",
    "react": "^19.1.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "husky": "^9.1.7",
    "tsx": "^4.20.3"
  }
}
